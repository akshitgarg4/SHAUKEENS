				</div>
		</div>

		<div class="queue">
			<div class="content">
				<div class="border">
				</div>
				<div class="heading">
					<p>PLAY LIST</p>
				</div>
				<div class="scroll playlist-section">
					<% for(song of songs) { %>
						<% if(song.dest == "plist") { %>
					<div class="list <%=song.origin+song.name%>">
						 <div class="songs"> 
							 <div><%= song.song %></div>
						 </div>
						 <div class="song-icons add" data-name="<%= song.name %>" data-origin="<%= song.origin %>" data-root="false"><i class="fas fa-minus-circle"></i></div>
						 <div class="song-icons heart"  data-name="<%= song.name %>" data-origin="<%= song.origin %>" data-root="true"><i class="fas fa-heart"></i></div>
						 <div class="song-icons"><i class="far fa-play-circle" id="playpausebtn" data-src="<%= song.src %>"></i></div>
					</div>
					<% } %>
					<% } %>
				
				</div>
			</div>
			
			<div class="content">
				<div class="border">
				</div>
				<div class="heading">
